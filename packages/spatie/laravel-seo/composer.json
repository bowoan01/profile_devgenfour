    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "Spatie\\LaravelSeo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\LaravelSeo\\LaravelSeoServiceProvider"
            ],
